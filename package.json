        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "gulp-clean": "^0.4.0",
        "gulp-terser": "^2.0.1",
        "html-webpack-plugin": "^5.3.1",
        "husky": "^5.2.0",
        "mini-css-extract-plugin": "^1.3.9",
        "prettier": "^2.2.1",
        "prettier-plugin-packagejson": "^2.2.10",
