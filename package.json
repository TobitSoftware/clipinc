        "@babel/preset-react": "^7.22.5",
        "@emotion/babel-plugin": "^11.11.0",
        "@types/jest": "^29.5.4",
        "@typescript-eslint/eslint-plugin": "^6.4.1",
        "@typescript-eslint/parser": "^6.4.1",
        "archiver": "^5.3.0",
        "babel-eslint": "^10.1.0",
